{
  "name": "starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios --device",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "start": "expo start --dev-client",
    "test": "jest",
    "clean": "cd android; ./gradlew clean; cd ..",
    "pod": "cd ios; pod install; cd ..",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle-ios": "npx react-native bundle --entry-file ./index.js --platform ios --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-apk-debug": "cd android; ./gradlew assembleDebug; cd ..",
    "build-apk-release": "yarn run bundle-android;  cd android; ./gradlew assembleRelease; cd ..",
    "build-aab": "yarn run bundle-android;  cd android; ./gradlew bundleRelease; cd ..",
    "postinstall": "husky install && patch-package",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx,json}": [
      "eslint --fix"
    ],
    "index.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.6",
    "@notifee/react-native": "^7.7.1",
    "@react-native-firebase/app": "^17.4.3",
    "@react-native-firebase/crashlytics": "^17.4.3",
    "@react-native-firebase/messaging": "^17.4.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/flash-list": "^1.4.3",
    "async-mutex": "^0.4.0",
    "dayjs": "^1.11.7",
    "expo": "~48.0.18",
    "expo-image": "^1.2.3",
    "expo-localization": "~14.1.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "i18n-js": "^4.2.3",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^2.8.0",
    "react-native-modal-dropdown": "^1.0.2",
    "react-native-pressable-scale": "^2.1.0",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.2",
    "react-native-screens": "^3.20.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/i18n-js": "^3.8.4",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-modal-dropdown": "^1.0.1",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "0.73.9",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.3",
    "react-native-flipper": "^0.190.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2",
    "typescript": "4.8.4"
  },
  "prettier": {
    "bracketSameLine": false,
    "bracketSpacing": true,
    "printWidth": 90,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "react-native"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-application",
        "expo-constants",
        "expo-file-system",
        "expo-font",
        "expo-keep-awake"
      ]
    }
  }
}
